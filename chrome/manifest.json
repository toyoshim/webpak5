{
  "manifest_version": 2,
  "name": "webpak5",
  "description": "wpak32 compatible mp3 player in HTML5",
  "version": "0.4.0",
  "app": {
    "background": {
      "scripts": [ "launch.js" ]
    }
  },
  "icons": {
    "16": "icon_016.png",
    "128": "icon_128.png"
  },
  "file_handlers": {
    "Audio": {
      "types": [ "audio/*" ]
    }
  },
  "commands": {
    "Z": {
      "suggested_key": {
        "default": "Ctrl+Z"
      },
      "description": "◀︎◀︎"
    },
    "S": {
      "description": "■"
    },
    " ": {
      "description": "▶"
    },
    "X": {
      "description": "||"
    },
    "C": {
      "suggested_key": {
        "default": "Ctrl+C"
      },
      "description": "▶▶"
    },
    "N": {
      "suggested_key": {
        "default": "Ctrl+N"
      },
      "description": "|◀︎"
    },
    "M": {
      "suggested_key": {
        "default": "Ctrl+M"
      },
      "description": "|▶︎"
    }
  }
}
